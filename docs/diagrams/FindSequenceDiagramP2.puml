@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":FindCommandParser" as FindCommandParser LOGIC_COLOR
participant ":ParserUtil" as ParserUtil LOGIC_COLOR
participant "f:FindCommand" as FindCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "predicate:NameContainsKeywordsPredicate" as NameContainsKeywordsPredicate MODEL_COLOR
end box

group sd create predicate object and FindCommand object
    activate FindCommandParser
    FindCommandParser -> ParserUtil : parseNamePredicate(argMultimap.getValue(PREFIX_NAME).get())
    activate ParserUtil

    create NameContainsKeywordsPredicate
    ParserUtil -> NameContainsKeywordsPredicate
    activate NameContainsKeywordsPredicate

    NameContainsKeywordsPredicate --> ParserUtil : predicate
    deactivate NameContainsKeywordsPredicate

    ParserUtil --> FindCommandParser : predicate
    deactivate ParserUtil

    create FindCommand
    FindCommandParser -> FindCommand : new FindCommand(predicate)
    activate FindCommand

    FindCommand --> FindCommandParser : f
    deactivate FindCommand
end

@enduml
