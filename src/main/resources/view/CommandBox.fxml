<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>

<StackPane styleClass="stack-pane" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
  <TextField fx:id="commandTextField" onAction="#handleCommandEntered" promptText="Enter command here..." />
   <Button fx:id="clearButton" alignment="CENTER" minHeight="0.0" minWidth="0.0" mnemonicParsing="false" onAction="#clearCommandBox" prefHeight="40.0" prefWidth="40.0" StackPane.alignment="CENTER_RIGHT" />
</StackPane>
