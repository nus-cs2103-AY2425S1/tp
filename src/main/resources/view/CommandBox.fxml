<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>

<StackPane styleClass="command-holder" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
  <TextField fx:id="commandTextField" onAction="#handleCommandEntered" promptText="Enter command here..." stylesheets="@Extensions.css">
      <styleClass>
         <String fx:value="focusable" />
         <String fx:value="command-bar" />
      </styleClass></TextField>
</StackPane>
