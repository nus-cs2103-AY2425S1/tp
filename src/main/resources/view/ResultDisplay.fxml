<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="placeHolder" styleClass="pane-with-border" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
  <ScrollPane fx:id="scrollPane" fitToWidth="true" maxHeight="100" styleClass="result-display">
      <Label fx:id="resultDisplay" wrapText="true" prefWidth="950"
             style="-fx-background-color: #EDF8ED; -fx-line-spacing: 5; -fx-font-size: 13"/>
  </ScrollPane>
</StackPane>
