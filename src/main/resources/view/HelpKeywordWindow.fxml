<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>

<fx:root resizable="false" title="Help Page" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
    <icons>
        <Image url="@/images/help_icon.png" />
    </icons>
   <scene>
      <Scene>
         <stylesheets>
                 <URL value="@HelpKeywordWindow.css" />
         </stylesheets>
         <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" prefHeight="400.0" prefWidth="500.0">
            <content>
               <VBox fx:id="helpKeywordContainer" maxHeight="1.7976931348623157E308" spacing="10.0">
                  <padding>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </padding>
                  <children>
                     <Label fx:id="header" alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="500.0" minHeight="-Infinity" text="Label" wrapText="true" />
                     <Label fx:id="description" alignment="TOP_LEFT" lineSpacing="2.0" maxHeight="1.7976931348623157E308" maxWidth="500.0" minHeight="-Infinity" text="Label" wrapText="true" />
                     <Label fx:id="parametersHeader" alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="500.0" minHeight="-Infinity" text="Label" wrapText="true" />
                     <Label fx:id="parameters" alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="500.0" minHeight="-Infinity" text="Label" wrapText="true" />
                     <Label fx:id="usageHeader" alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="500.0" minHeight="-Infinity" text="Label" wrapText="true" />
                     <Label fx:id="usage" alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="500.0" minHeight="-Infinity" text="Label" wrapText="true" />
                     <Label fx:id="exampleHeader" alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="500.0" minHeight="-Infinity" text="Label" wrapText="true" />
                     <Label fx:id="example" alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="500.0" minHeight="-Infinity" text="Label" wrapText="true" />
                  </children>
               </VBox>
            </content>
         </ScrollPane>
      </Scene>
   </scene>
</fx:root>
