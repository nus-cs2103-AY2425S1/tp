<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<?import java.net.URL?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" VBox.vgrow="ALWAYS">
  <stylesheets>
    <URL value="@DarkTheme.css" />
  </stylesheets>
  <TextArea fx:id="weddingLabel" editable="false" minWidth="50" maxHeight="24.0" prefHeight="24.0" prefWidth="512.0" styleClass="panel-title" text="Weddings" wrapText="true" VBox.vgrow="NEVER">
    <font>
      <Font name="System Bold" size="20.0" />
    </font></TextArea>
  <ListView fx:id="weddingListView" VBox.vgrow="ALWAYS" />
</VBox>
