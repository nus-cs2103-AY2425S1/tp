<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>

<fx:root title="View" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
  <scene>
    <Scene>
        <VBox alignment="CENTER_LEFT" minHeight="105" >

          <Label fx:id="name" wrapText="true" />
          <FlowPane fx:id="tags" />
          <Label fx:id="phone" wrapText="true" />
          <Label fx:id="address" wrapText="true" />
          <Label fx:id="email" wrapText="true" />
          <Label fx:id="dateOfLastVisit" wrapText="true" />
          <Label fx:id="emergencyContact" wrapText="true" />
          <Label fx:id="remark" wrapText="true" />
          <padding>
            <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
          </padding>
        </VBox>
    </Scene>
  </scene>
</fx:root>
